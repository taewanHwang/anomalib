{
  "experiment_conditions": [
    {
      "name": "reverse_distillation_quick_test",
      "description": "Reverse Distillation 빠른 테스트용 - 기본 설정으로 작은 배치",
      "config": {
        "batch_size": 16,
        "image_size": "224x224",
        "source_domain": "domain_A",
        "target_domains": "auto",
        "backbone": "wide_resnet50_2",
        "layers": ["layer1", "layer2", "layer3"],
        "pre_trained": true,
        "anomaly_map_mode": "multiply",
        "max_epochs": 10,
        "early_stopping_patience": 5,
        "learning_rate": 0.005,
        "optimizer": "adam",
        "weight_decay": 0.0001
      }
    }
  ]
}
